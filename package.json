{
  "name": "Mon-site-web-Back",
  "version": "0.0.1",
  "description": "Back End serveur web",
  "type": "module",
  "main": "src/server.js",
  "repository": {
    "type": "git",
    "url": "https://nexus.asap.dsna.fr/repository/npm-group/"
  },
  "publishConfig": {
    "registry": "https://nexus.asap.dsna.fr/repository/npm-releases/"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "cache": false
  },
  "scripts": {
    "lint": "`npm bin`/eslint src/ --ext .js --ext .jsx",
    "test": "NODE_ENV=production mocha --es-module-specifier-resolution=node --timeout 20000 tests/**/*.spec.js",
    "test:watch": "npm t -- -w",
    "cover": "nyc npm t",
    "launch": "nodemon --es-module-specifier-resolution=node src/server.js",
    "dev": "DEBUG=* npm run launch",
    "start": "NODE_ENV=production npm run launch"
  },
  "author": "DSNA/DTI, Luc Thibault",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^4.0.3",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "mongoose": "^5.9.1",
    "socket.io": "^2.3.0",
    "uuid": "^8.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0"
  }
}
